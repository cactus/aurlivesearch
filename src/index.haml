!!!
%html
  %head
    %meta(name="robots" content="noindex,nofollow")
    %link(href="/static/blueprint/screen.css" rel="stylesheet" 
          type="text/css" media="screen, projection")
    %link(href="/static/blueprint/print.css" rel="stylesheet" 
          type="text/css" media="print")
    /[if IE]
      %link(href="/static/blueprint/ie.css" rel="stylesheet" 
            type="text/css" media="screen, projection")
    %link(href="/compiled/screen.0.2.css" rel="stylesheet" type="text/css")
    %script#packages_tpl(type="text/html")
      :plain
        <ul>{{#packages}}<li>{{#pkg}}
        {{Name}}:
        <ul>
          <li>version: {{Version}}</li>
          <li>pkgid: {{ID}}</li>
          <li>votes: {{NumVotes}}</li>
          <li>url: <a href="{{URL}}">{{URL}}</a></li>
          <li>aur-tarball: <a href="http://aur.archlinux.org{{URLPath}}">{{URLPath}}</a></li>
          <li>description: {{Description}}</li>
        </ul><br>
        {{/pkg}}</li>{{/packages}}</ul>
    %script#error_tpl(type="text/html")
      :plain
        <div id="error">{{short_msg}}<br>{{long_msg}}</div>
  %body
    .container
      #counter.span-9
        results:
        %span#result-count 0
      #searchform.span-6
        .span-5
          %form
            %label(for="q") Search
            %input#q(name="q" type="text")
        #ajax-loading.span-1.last
          %img(src="/static/ajax-loader.gif" width="16" height="16")
      #links.span-9.last
        %a.quiet(href="https://github.com/cactus/aurlivesearch") about
      %hr.space
      #response.span-24.last
    %script(src="http://ajax.googleapis.com/ajax/libs/jquery/1.5.0/jquery.min.js" type="text/javascript")
    %script(src="/static/ICanHaz.min.js" type="text/javascript")
    %script(src="/compiled/search.0.2.js" type="text/javascript")
